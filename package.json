{
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "description": "Current SASS Project: Creativia",
  "homepage": "https://greycat20142017.github.io/creativia",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "8.3.x",
    "browser-sync": "^2.24.7",
    "del": "^3.0.0",
    "gulp": "^4.0.0",
    "gulp-bem-css": "^1.1.3",
    "gulp-csso": "^3.0.1",
    "gulp-imagemin": "^4.1.0",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "7.0.x",
    "gulp-posthtml": "^3.0.4",
    "gulp-rename": "^1.2.2",
    "gulp-run-sequence": "^0.3.2",
    "gulp-sass": "^4.0.1",
    "gulp-svgstore": "^6.1.1",
    "gulp-uglify": "^3.0.0",
    "gulp-webp": "^3.0.0",
    "posthtml-include": "^1.1.0",
    "stylelint": "9.2.x",
    "stylelint-config-htmlacademy": "0.1.x"
  },
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.scss"
  ],
  "engines": {
    "node": "8.11"
  },
  "private": true,
  "scripts": {
    "build": "gulp build",
    "editorconfig": "editorconfig-cli",
    "serve": "gulp serve",
    "start": "npm run build && npm run serve",
    "stylelint": "stylelint \"creativia/source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d creativia/build"
  },
  "version": "14.0.0",
  "dependencies": {
    "gh-pages": "^1.2.0",
    "hoek": "^5.0.4"
  }
}
